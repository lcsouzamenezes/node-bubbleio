{
  "name": "node-bubbleio",
  "version": "0.0.5",
  "description": "Bubble.io Data and Workflow API client for node.js",
  "keywords": [
    "bubbleio",
    "bubble",
    "node"
  ],
  "license": "MIT",
  "repository": "https://github.com/pixelplicity/node-bubbleio",
  "homepage": "https://github.com/pixelplicity/node-bubbleio#readme",
  "type": "module",
  "source": "src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "exports": {
    "require": "./dist/index.cjs",
    "default": "./dist/index.modern.js"
  },
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "files": [
    "src",
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "shx rm -rf dist/ && microbundle --target node --tsconfig ./tsconfig.json",
    "dev": "microbundle watch"
  },
  "author": "Curtis Cummings <curtis@pixelplicity.com>",
  "devDependencies": {
    "@types/lodash.omit": "^4.5.6",
    "@types/node-fetch": "^2.5.12",
    "@types/qs": "^6.9.7",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "microbundle": "^0.14.1",
    "prettier": "^2.4.1",
    "shx": "^0.3.3",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "lodash.omit": "^4.5.0",
    "node-fetch": "^2.6.5",
    "qs": "^6.10.1"
  }
